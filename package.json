    "@babel/core": "^7.26.0",
    "@testing-library/jest-native": "^5.4.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^19.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.9",
